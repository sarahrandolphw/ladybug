--npcs
npcs={}
local spawn_time=0
local interval = 300

function init_npc()
	local new = {x=7*8,y=8*8,sprite=36, direction=2}
 add(npcs,new)  
end

function draw_npcs()
	for _,npc in pairs(npcs) do
		spr(npc.sprite,npc.x,npc.y)
	end 
end
		

function npc_collision_check()
	for _, npc in pairs(npcs) do
		if p.x<npc.x+8 and p.x+8>npc.x and p.y<npc.y+8 and p.y+8>npc.y then
			sfx(1)
			lose_life()
		end
	end
end

function npc_update()
 spawn_time += 1
 if spawn_time >= interval and #npcs<4 then
  init_npc()
  spawn_time = 0
 end
 for _, npc in pairs(npcs) do
 	newx=npc.x
 	newy=npc.y
 	if npc.direction==0 then
			newx-=1
			npc.sprite=36			
		elseif npc.direction==1 then
			newx+=1
			npc.sprite=35
		elseif npc.direction==2 then
			newy-=1
			npc.sprite=32
		elseif npc.direction==3 then
			newy+=1
			npc.sprite=37
		end
		if can_move(newx,newy) then
			npc.x=newx
			npc.y=newy
		else
			npc.direction=flr(rnd(4))
		end	
	end
end

		

