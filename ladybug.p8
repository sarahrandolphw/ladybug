--map/flowers
flowers={}
flowers_collected=0
total_flowers=0

function place_flr()
	for x=1,13 do
		for y=1,13 do
			if fg(x,y)==0 then
				add(flowers,{x=x,y=y})
				total_flowers+=1
			end
		end
	end
end

function draw_flr()
	--spr(14,3*8,12*8)
	for flowers in all(flowers) do 
		spr(46,flowers.x*8,flowers.y*8)
	end
end

function collect_flower()
	for i,flower in pairs(flowers) do
		if p.x<flower.x*8+8 and p.x+8>flower.x*8 and
					p.y<flower.y*8+8 and p.y+8>flower.y*8 then
			del(flowers,flower)
			flowers_collected+=1
			sfx(0)
			check_level()
		end
	end					
end

function check_level()
	if flowers_collected==total_flowers then
		sfx(2)
		mode=1
	end
end

