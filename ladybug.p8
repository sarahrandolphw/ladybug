--pc
function p_init()
 p={}
 p.x,p.y=7*8,8*8 --position (starting on a grid)
 p.dx,p.dy=0,0 --movement
 p.w,p.h=8,8   --hitbox size matching grid size
 p.d=0         --direction
 p.walking=false
 p.spd=1       --speed (in grid steps)
 p.spr=0
 p.flp=false
 p.lives=3
 p.boost=false
 p.stamina=3
end


local power_time=0
local power_int = 100

function lose_life()
	if p.lives<=1 then
		sfx(3)
		mode=2
	else
		p.lives-=1
		p.x,p.y=7*8,8*8
		p.spr=0
		p.walking=false
	end
end

function move_player()
 power_time+=1
 if power_time>=power_int then
 	p.spd=1
 	p.boost=false
 end
 if p.walking then
  p.x += p.dx
  p.y += p.dy
  -- stop walking when aligned to the grid
  if p.x % 8 == 0 and p.y % 8 == 0 then
   p.walking = false
   p.dx, p.dy = 0, 0
  end
 else
  if btn(0) then
   p.dx = -p.spd
   p.d = 0
   p.spr = 1
   p.flp = true
   p.walking = true
   if p.boost then
   	p.spr=17
   end
  elseif btn(1) then
   p.dx = p.spd
   p.d = 1
   p.spr = 1
   p.flp = false
   p.walking = true
   if p.boost then
   	p.spr=17
   end
  elseif btn(2) then
   p.dy = -p.spd
   p.d = 2
   p.spr = 0
   p.walking = true
   if p.boost then
   	p.spr=16
   end
  elseif btn(3) then
   p.dy = p.spd
   p.d = 3
   p.spr = 2
   p.walking = true
   if p.boost then
   	p.spr=18
   end
  elseif btnp(❎) and p.stamina>0 then
  	p.spd=2
  	p.boost=true
  	power_time=0
  	p.stamina-=1
  end

  if p.walking and not can_move(p.x + p.dx, p.y + p.dy) then
   p.walking = false
   p.dx, p.dy = 0, 0
  end
 end
end

-- draw the player sprite
function draw_player()
 spr(p.spr, p.x, p.y, 1, 1, p.flp)
end

function can_move(x, y)
 local x1, y1 = flr(x/8), flr(y/8)
 local x2, y2 = flr((x+7)/8), flr((y+7)/8)
 if fg(x1, y1) != 0 or fg(x2, y1) != 0 or fg(x1, y2) != 0 or fg(x2, y2) != 0 then
   return false
 end
 return true
end

function fg(x, y) 
 local flag = fget(mget(x, y))
 return flag
end


